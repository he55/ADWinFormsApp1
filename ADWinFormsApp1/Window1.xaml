        <Button Content="Text" HorizontalAlignment="Left" Margin="252,54,0,0" VerticalAlignment="Top" Click="Button_Click_2" Width="51"/>
        <Button Content="URL" HorizontalAlignment="Left" Margin="252,93,0,0" VerticalAlignment="Top" Click="Button_Click_3" Width="51"/>
